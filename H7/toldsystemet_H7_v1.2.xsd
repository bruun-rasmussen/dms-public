												<xs:element name="LineNumeric" type="ds:PreviousDocumentLineNumericType" minOccurs="1"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="UCR" minOccurs="0" maxOccurs="1">
										<xs:annotation>
											<xs:documentation>
												<type>EUCDM 5.2</type>
